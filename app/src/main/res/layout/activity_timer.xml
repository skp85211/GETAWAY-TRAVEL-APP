<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00000000"
    tools:context=".timer">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="277dp"
        android:layout_height="98dp"
        android:layout_marginTop="156dp"
        android:fontFamily="cursive"
        android:gravity="center"
        android:text="Get-A-Way"
        android:textColor="#EE7001"
        android:textSize="50sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView8"
        app:layout_constraintHorizontal_bias="0.166"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toTopOf="@+id/imageView8" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="271dp"
        android:layout_height="111dp"
        android:layout_marginTop="312dp"
        android:fontFamily="cursive"
        android:text="Adventure Awaits, Go Find It!"
        android:textColor="#EE7001"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/imageView8" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="241dp"
        android:layout_height="169dp"
        android:layout_marginTop="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.429"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logoapp" />

    <SeekBar
        android:id="@+id/seekBar2"
        android:layout_width="360dp"
        android:layout_height="109dp"
        android:layout_marginTop="368dp"
        android:progress="@android:integer/config_shortAnimTime"
        android:progressBackgroundTint="#EE7001"
        android:progressTint="#EE7001"
        android:thumb="@drawable/ic_plane"
        app:layout_constraintEnd_toEndOf="@+id/imageView8"
        app:layout_constraintHorizontal_bias="0.403"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toTopOf="@+id/imageView8" />
</androidx.constraintlayout.widget.ConstraintLayout>